# Configurações de Produção - BikeFix Frontend

# URL da API Backend em Produção
REACT_APP_API_URL=https://bikefix-backend.onrender.com/api

# URLs de Callback para OAuth
REACT_APP_GOOGLE_CALLBACK_URL=https://bikefix-frontend.onrender.com/auth/callback
REACT_APP_STRAVA_CALLBACK_URL=https://bikefix-frontend.onrender.com/auth/strava/callback

# Configurações do Mapa
REACT_APP_MAP_DEFAULT_LAT=-23.5505
REACT_APP_MAP_DEFAULT_LNG=-46.6333
REACT_APP_MAP_DEFAULT_ZOOM=12

# Configurações de Upload
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# Configurações de Performance
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true

# Configurações de Cache
REACT_APP_CACHE_DURATION=300000

# Ambiente
REACT_APP_ENVIRONMENT=production